   "source": [
    "from sklearn import tree\n",
    "import graphviz\n",
    "\n",
    "X = [[1, 1], [2, 1], [1, 2], [2, 2]]\n",
    "y = [0, 1, 1, 1]\n",
    "clf = tree.DecisionTreeClassifier()\n",
    "clf.fit(X, y)\n",
    "\n",
    "dot_data = tree.export_graphviz(clf, filled = True)\n",
    "graph = graphviz.Source(dot_data)\n",
    "graph"
   ]
